{
  "name": "meme-party",
  "version": "2.0.0",
  "private": true,
  "author": "Matthew Espino",
  "description": "A fun site that shows random memes from reddit",
  "keywords": [
    "memes",
    "reddit"
  ],
  "license": "MIT",
  "scripts": {
    "log:serve": "cross-env NODE_ENV=development USE_CASE=serve LOG_RUN=true node webpack-log.config.js",
    "log:dev": "cross-env NODE_ENV=development USE_CASE=dev LOG_RUN=true node webpack-log.config.js",
    "log:prod": "cross-env NODE_ENV=production USE_CASE=prod LOG_RUN=true node webpack-log.config.js",
    "start": "cross-env NODE_ENV=development USE_CASE=serve webpack serve",
    "build:dev": "cross-env NODE_ENV=development USE_CASE=dev webpack --progress",
    "build:prod": "cross-env NODE_ENV=production USE_CASE=prod webpack --progress"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^1.2.1",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.0.4",
    "remove-files-webpack-plugin": "^1.4.3",
    "sass": "^1.26.9",
    "sass-loader": "^10.0.4",
    "webpack": "^5.3.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^4.0.0-beta.0"
  },
  "browserslist": "defaults",
  "sideEffects": false
}
